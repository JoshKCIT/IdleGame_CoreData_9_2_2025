  "upgrades": {
    "duration": {
      "description": "Increases the amount of time Golden Bot is active. Duration starts at 20 seconds and has 30 levels, with each level increasing the duration by 0.5 seconds. The maximum duration is 35 seconds without any labs. This can be further increased to 45 seconds with labs.",
      "max_levels": 30,
      "base_value": 20,
      "increment_per_level": 0.5,
      "max_without_labs": 35,
      "max_with_labs": 45
    },
    "cooldown": {
      "description": "The amount of time between initial activations. Cooldown starts at 120 seconds and has 15 levels, with each level decreasing the cooldown by 3 seconds. The minimum cooldown is 75 seconds without any labs. This can be further reduced to 50 seconds with labs.",
      "max_levels": 15,
      "base_value": 120,
      "increment_per_level": -3,
      "min_without_labs": 75,
      "min_with_labs": 50
    },
    "bonus": {
      "description": "Increases the coin bonus from enemies destroyed. Bonus starts at 2.0x bonus and has 30 levels, with each level increasing the bonus by 0.2x. The maximum bonus is 8.0x.",
      "max_levels": 30,
      "base_value": 2.0,
      "increment_per_level": 0.2,
      "max_bonus": 8.0
    },
    "range": {
      "description": "Increases the range the bot covers when active. Range is amplified by tower range. Ranges starts at 20m and has 15 levels, with each level increasing the range by 2m. The maximum range from these upgrades is 50m. This can be further increased with relics and vault upgrades. Additionally, the range of all bots is also amplified based on the tower's range.",
      "max_levels": 15,
      "base_value": 20,
      "increment_per_level": 2,
      "max_range": 50
    }
  },
  "notes": "Additional information",
  "unlocking_upgrading": {
    "description": "Like all bots, Golden Bot is unlocked and upgraded in the Event Shop menu. The unlock cost will depend on how many other bots have been purchased. Each of the 4 Golden Bot upgrades start with a base upgrade cost of 100 medals and increase by 40 medals each level."
  },
